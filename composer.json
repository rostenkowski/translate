{
  "name": "rostenkowski/translate",
  "type": "library",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "ext-intl": "*",
    "ext-tokenizer": "*",
    "nette/di": "^3.0",
    "nette/neon": "^3.0",
    "nette/safe-stream": "^2.3",
    "nette/utils": "^3.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "nette/tester": "^2.0",
    "mockery/mockery": "^1.0@dev",
    "phpstan/phpstan": "^0.9.0@dev"
  },
  "suggest": {
    "monolog/monolog": "To log translation errors."
  },
  "autoload": {
    "psr-4": {
      "Rostenkowski\\": [
        "src"
      ]
    }
  }
}
